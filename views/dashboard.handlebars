<section>
    {{#each }}
    <div class="card-header">
      <div class="title ">
          <h2><a href="/blog/{{blog.id}}">{{blog.title}}</a></h2>
      </div>
      <div>
          <p>posted by {{user.username}} on {{format_date blog.date_created}}</p>
      </div>
      <div class="btn">
          <a href="/dashboard" class="update">Update</a>
          <a href="/dashboard" class="delete">Delete</a>
      </div>
   </div>
    {{/each}}

    <div>
        <button class="btn">+ New Post</button>
    </div>
</section>